{
	"include": [
		"./src/**/*.ts"
	],
	"compilerOptions": {
		"outDir": "./built",
		"rootDir": "./src",
		"target": "es2017",
		"module": "commonjs",
		"lib": [
			"es2017"
		],
		"sourceMap": true,
		"strict": false,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true
	}
}
